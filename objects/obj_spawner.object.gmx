if(!instance_exists(par_enemy)) {
    nextRound = true;
    spawn = spawnTimer + 1;
}

if(nextRound &amp;&amp; global.roundIndex &lt; array_length_1d(allRounds)) {
    currentRound = allRounds[global.roundIndex];
    totalLeft = 0;
    for(i = 0; i &lt; array_length_1d(currentRound); i++) {
        totalLeft += currentRound[i];
    }
    global.roundIndex++;
    nextRound = false;
}

if(spawn &gt;= spawnTimer &amp;&amp; totalLeft &gt; 0) {
    var angle = random_range(0, 360);
    var xPos = (room_height / 2) + (450 * dcos(angle));
    var yPos = (room_height / 2) + (450 * dsin(angle));
    var enemyIndex = irandom_range(0, array_length_1d(enemies) - 1);
    while(currentRound[enemyIndex] == 0) {
        enemyIndex = irandom_range(0, array_length_1d(enemies) - 1);
    }
    var enemy = instance_create(xPos, yPos, enemies[enemyIndex]);
    if(enemyIndex == 1) {
        show_debug_message("1");
        enemy.spiral = true;
        enemy.spiralSign = choose(-1, 1);
    }
    else if(enemyIndex == 2) {
        show_debug_message("2");
        enemy.zigzag = true;
    }
    totalLeft--;
    currentRound[enemyIndex]--;
    spawn = 0;
    spawnTimer = irandom_range(.5 * room_speed, 2 * room_speed);
}
else if(spawn &lt; spawnTimer) {
